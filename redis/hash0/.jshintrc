{
  "node":          true,
  "bitwise":       true,
  "camelcase":     true,
  "curly":         true,
  "eqeqeq":        true,
  "forin":         true,
  "freeze":        true,
  "immed":         true,
  "indent":        2,  
  "latedef":       "nofunc",
  "newcap":        true,
  "noarg":         true,
  "noempty":       true,
  "nonew":         true,
  "plusplus":      false,
  "quotmark":      "single",
  "undef":         true,
  "unused":        true,
  "strict":        true,
  "trailing":      true,
  "maxcomplexity": 10,
  "globals": {
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false
  }
}
